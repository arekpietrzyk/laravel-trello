{
  "name": "gregoriohc/laravel-trello",
  "description": "A Laravel wrapper and facade package for the Trello API",
  "keywords": ["laravel", "package", "facade", "trello", "api"],
  "homepage": "https://github.com/gregoriohc/laravel-trello",
  "license": "MIT",
  "authors": [
    {
      "name": "Gregorio Hern√°ndez Caso",
      "email": "gregoriohc@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/config": "~5",
    "illuminate/support": "~5",
    "cdaguerre/php-trello-api": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Gregoriohc\\LaravelTrello\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Gregoriohc\\LaravelTrello\\TrelloServiceProvider"
      ]
    }
  }
}
